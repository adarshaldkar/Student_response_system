# Production environment variables for responsive fixes
GENERATE_SOURCEMAP=false
REACT_APP_BUILD_MODE=production

# Backend API URL - Points to Render deployed backend
REACT_APP_BACKEND_URL=https://student-response-system.onrender.com
REACT_APP_API_URL=https://student-response-system.onrender.com/api

# Google OAuth Configuration
# Replace with your actual Google OAuth Client ID from Google Cloud Console
REACT_APP_GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID_HERE

# Ensure CSS optimization doesn't break responsive classes
REACT_APP_CSS_MODULES=false
